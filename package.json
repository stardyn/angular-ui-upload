{
  "name": "@stardyn/angular-ui-upload",
  "version": "2.0.9",
  "description": "Angular UI Upload Package - File upload component with progress tracking and error handling for Angular applications",
  "main": "core.js",
  "module": "core.js",
  "browser": "core.js",
  "umd": "core.js",
  "types": "main.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pwsh -File ./build.ps1",
    "build:prod": "pwsh -File ./build.ps1 -Production",
    "build:npmjs": "pwsh -File ./build_publish_npmjs.ps1",
    "build:private": "pwsh -File ./build_publish.ps1",
    "publish:npmjs": "pwsh -File ./build_publish_npmjs.ps1 -Environment live",
    "publish:private": "pwsh -File ./build_publish.ps1 -Environment live",
    "publish:dry": "pwsh -File ./build_publish.ps1 -DryRun",
    "publish:skip": "pwsh -File ./build_publish.ps1 -SkipBuild",
    "clean": "rimraf dist",
    "dev": "pwsh -File ./build.ps1 -Environment dev",
    "test": "echo 'No tests specified'",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "~5.5.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4",
    "ts-loader": "^9.4.0",
    "terser-webpack-plugin": "^5.3.0",
    "rimraf": "^5.0.0"
  },
  "dependencies": {
    "@types/fine-uploader": "^5.14.0",
    "fine-uploader": "^5.16.2"
  },
  "peerDependencies": {
    "@angular/common": ">=16.0.0",
    "@angular/core": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "angular",
    "ui-components",
    "upload",
    "file-upload",
    "progress",
    "typescript",
    "stardyn",
    "angular-ui"
  ],
  "author": "Stardyn Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stardyn/angular-ui-upload.git"
  },
  "homepage": "https://github.com/stardyn/angular-ui-upload",
  "bugs": {
    "url": "https://github.com/stardyn/angular-ui-upload/issues"
  }
}